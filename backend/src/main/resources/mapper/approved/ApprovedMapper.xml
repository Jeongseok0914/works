<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jeongseok.demoweb.dao.approved.ApprovedDao">
	<resultMap id="approvedMap" type="com.jeongseok.demoweb.vo.approved.ApprovedVo">
		<result property="title" column="title"/>
		<result property="content" column="content"/>
		<result property="status" column="status"/>
		<result property="delYn" column="del_yn"/>
		<result property="approvedUserId" column="approved_user_id"/>
		<result property="sysCretDate" column="sys_cret_date"/>
		<result property="sysCretUserId" column="sys_cret_user_id"/>
		<result property="sysUpdtDate" column="sys_updt_date"/>
		<result property="sysUpdtUserId" column="sys_updt_user_id"/>
    </resultMap>
    
    <insert id="insertApprovedInfo" parameterType="com.jeongseok.demoweb.controller.approved.ApprovedParam">
    	INSERT INTO approved_info(
    		title
    		, content
    		, status
    		, del_yn
    		, approved_user_id
    		, sys_cret_date
    		, sys_cret_user_id
    		, sys_updt_date
    		, sys_updt_user_id
    		) VALUES (
    		  #{title}
    		, #{content}
    		, #{status}
    		, 'N'
    		, #{approvedUserId}
    		, sysdate()
    		, #{sysCretUserId}
    		, NULL
    		, NULL);
    
    </insert>
</mapper>